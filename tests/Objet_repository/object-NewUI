 /////-----/-----Object Playout LIVE STUDIO-----////////----///

//Playlist date picker button
PlaylistDatePicker = page.locator('.react-datepicker__input-container');

//Overlay Playlist date picker
scrollContainer = page.getByRole('dialog', { name: 'Choose Date and Time' });

//Option date
